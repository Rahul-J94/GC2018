		//alert(locations);
function map(){

//Create the map
var map = new google.maps.Map(document.getElementById("map_div"), {
    center: new google.maps.LatLng(-27.511738, 153.151594),
    zoom: 8,
    mapTypeId: google.maps.MapTypeId.ROADMAP
});

var n = 1;
var m = "marker";

for( i = 0; i < locations.length; i++ ) {
	var position = new google.maps.LatLng(locations[i][0], locations[i][1]);

	marker = new google.maps.Marker({
	position: position,
	map: map,
	id: n,
	icon: "http://chart.apis.google.com/chart?chst=d_map_pin_letter&chld=" + n +"|FF0000|FFFFFF"
});
allMarkers.push(marker);
    n++;
}


}


//Function called when hover the div
function hover(id) {
    for ( var i = 0; i< allMarkers.length; i++) {
        if (id === allMarkers[i].id) {
           //allMarkers[i].setIcon(icon);
			allMarkers[i].setAnimation(google.maps.Animation.BOUNCE);
           break;
        }
   }
}

//Function called when out the div
function out(id) {
    for ( var i = 0; i< allMarkers.length; i++) {
        if (id === allMarkers[i].id) {
          // allMarkers[i].setIcon(icon);
		   allMarkers[i].setAnimation(null);
           break;
        }
   }
}
function toggleBounce() {
        if (marker.getAnimation() !== null) {
          marker.setAnimation(null);
        } else {
          marker.setAnimation(google.maps.Animation.BOUNCE);
        }
      }
</script>

